  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "assert": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "google-auth-library": "^6.0.6",
